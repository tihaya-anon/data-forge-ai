# Copy this file to .env and fill in your values

# Application Environment
DATAFORGE_ENV=development
DATAFORGE_DEBUG=true

# Kafka Configuration
DATAFORGE_KAFKA__BOOTSTRAP_SERVERS=localhost:9092
DATAFORGE_KAFKA__TOPIC_PREFIX=dataforge

# Storage Configuration
DATAFORGE_STORAGE__MINIO_ENDPOINT=localhost:9000
DATAFORGE_STORAGE__MINIO_ACCESS_KEY=minioadmin
DATAFORGE_STORAGE__MINIO_SECRET_KEY=minioadmin123

# Redis Configuration
DATAFORGE_REDIS__HOST=localhost
DATAFORGE_REDIS__PORT=6379

# Milvus Configuration
DATAFORGE_MILVUS__HOST=localhost
DATAFORGE_MILVUS__PORT=19530

# Elasticsearch Configuration
DATAFORGE_ELASTICSEARCH__HOST=localhost
DATAFORGE_ELASTICSEARCH__PORT=9200
